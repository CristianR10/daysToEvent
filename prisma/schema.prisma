generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Registrations {
  id         String   @id @default(uuid())
  endpoint   String   @unique
  p256dh     String   @unique
  auth       String   @unique
  created_at DateTime @default(now())

  @@map("registrations")
}
